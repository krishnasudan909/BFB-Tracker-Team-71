<html>
  <script
    src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"
  ></script>
  <body>
    <canvas id="myChart" style="width:100%;max-width:700px"></canvas>

    <script>
      var xyValues = [ {x:50, y:7}, ]; new Chart("myChart", { type: "scatter",
      data: { datasets: [{ pointRadius: 4, pointBackgroundColor: "rgb(0,0,255)",
      data: xyValues }] }, options: { legend: {display: false}, scales: { xAxes:
      [{ticks: {min: 40, max:160}}], yAxes: [{ticks: {min: 6, max:16}}], } } });
    </script>

  </body>
</html>